{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "*.json", "*.ico"]
  },
  "mobileApiLocation": "api",
  "apiLocation": "api",
  "outputLocation": "dist/vacation-manager-frontend",
  "httpHeaders": [
    {
      "pattern": "*.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "pattern": "*.css",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "pattern": "index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ]
}
